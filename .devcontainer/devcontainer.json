{
    "secrets": {
        "TF_VAR_dbt_account_id": {
            "description": "dbt Cloud Account ID (can be found in Account Settings)"
        },
        "TF_VAR_dbt_token": {
            "description": "dbt Cloud API token secret needed to authenticate into the service.",
            "documentationUrl": "https://docs.getdbt.com/docs/dbt-cloud-apis/service-tokens"
        },
        "TF_VAR_dbt_host_url": {
            "description": "dbt Cloud API host URL (default https://cloud.getdbt.com/api"
        }
    },
    "name": "dbt Cloud Terraform Starter",
    "extensions": [
        "innoverio.vscode-dbt-power-user"
    ],
    "dockerFile": "Dockerfile",
    "settings": {
        "files.associations": {
            "*.tf": "hcl"
        }
    },
    "features": {
        "docker-in-docker": {
            "version": "latest",
            "moby": true,
            "dockerDashComposeVersion": "v1"
        }
    },
    "hostRequirements": {
        "cpus": 2,
        "memory": "4gb",
        "storage": "32gb"
    },
    "onCreateCommand": "",
    "postStartCommand": "/bin/sh startup.sh"
}